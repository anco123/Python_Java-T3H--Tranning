<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context https://www.springframework.org/schema/context/spring-context.xsd">

    <context:property-placeholder location="classpath*:db_javaParametter_JDBC.properties"/>

    <bean id="dbInfo" class="javaParametter_JDBC.preConfig.xml.model.DbInfo">
        <property name="" value="${db.url}"/>
        <property name="" value="${db.port}"/>
        <property name="" value="${db.username}"/>
        <property name="" value="${db.password}"/>
    </bean>
</beans>
